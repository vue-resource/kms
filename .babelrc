{
  "presets": [
    ["@babel/preset-env", {
      "modules": "commonjs",
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"],
        "node": "current"
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import", 
    "@babel/plugin-transform-runtime", 
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "compact": false
}